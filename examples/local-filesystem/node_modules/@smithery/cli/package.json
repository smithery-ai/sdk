{
	"name": "@smithery/cli",
	"version": "1.6.2",
	"type": "module",
	"private": false,
	"homepage": "https://smithery.ai/",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/smithery-ai/cli.git"
	},
	"description": "An NPX command to install and list Model Context Protocols from Smithery",
	"main": "dist/index.js",
	"scripts": {
		"build": "tsc && node build.mjs && shx chmod +x dist/index.js",
		"start": "node dist/index.js",
		"test:list": "tsx src/index.ts list",
		"test:install": "tsx src/index.ts install",
		"test:installed": "tsx src/index.ts installed",
		"extract": "tsx src/extractors/modelcontextprotocol-extractor.ts",
		"test:uninstall": "tsx src/index.ts uninstall",
		"prepare": "npm run build",
		"check": "npx @biomejs/biome check --write --unsafe",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"bin": {
		"smithery": "dist/index.js"
	},
	"dependencies": {
		"@anthropic-ai/mcpb": "^1.1.1",
		"@modelcontextprotocol/sdk": "^1.20.0",
		"@ngrok/ngrok": "^1.5.1",
		"@smithery/registry": "^0.4.2",
		"@smithery/sdk": "^1.7.3",
		"boxen": "^8.0.1",
		"chalk": "^4.1.2",
		"commander": "^14.0.0",
		"cors": "^2.8.5",
		"cross-fetch": "^4.1.0",
		"esbuild": "^0.25.10",
		"express": "^5.1.0",
		"inquirer": "^8.2.4",
		"inquirer-autocomplete-prompt": "^2.0.0",
		"lodash": "^4.17.21",
		"ora": "^8.2.0",
		"shx": "^0.4.0",
		"smol-toml": "^1.4.2",
		"uuid": "^11.1.0",
		"uuidv7": "^1.0.2",
		"yaml": "^2.3.4"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.4",
		"@types/cors": "^2.8.18",
		"@types/express": "^5.0.2",
		"@types/inquirer": "^8.2.4",
		"@types/inquirer-autocomplete-prompt": "^3.0.3",
		"@types/json-schema": "^7.0.15",
		"@types/lodash": "^4.17.14",
		"@types/node": "^20.19.11",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.2",
		"dotenv": "^17.2.2",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"ts-node": "^10.9.1",
		"tsx": "^4.19.2",
		"typescript": "^5.0.0",
		"vitest": "^3.2.4"
	},
	"peerDependencies": {
		"react": "^18.0.0 || ^19.0.0",
		"react-dom": "^18.0.0 || ^19.0.0"
	},
	"files": [
		"dist",
		"README.md",
		"package.json"
	],
	"exports": {
		".": {
			"import": "./dist/index.js"
		}
	},
	"engines": {
		"node": ">=20.0.0"
	}
}
