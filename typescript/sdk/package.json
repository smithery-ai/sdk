{
	"name": "@smithery/sdk",
	"version": "1.6.8",
	"description": "SDK to develop with Smithery",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./server": {
			"types": "./dist/server/index.d.ts",
			"default": "./dist/server/index.js"
		},
		"./server/stateful": {
			"types": "./dist/server/stateful.d.ts",
			"default": "./dist/server/stateful.js"
		},
		"./server/stateful.js": {
			"types": "./dist/server/stateful.d.ts",
			"default": "./dist/server/stateful.js"
		},
		"./server/stateless": {
			"types": "./dist/server/stateless.d.ts",
			"default": "./dist/server/stateless.js"
		},
		"./server/stateless.js": {
			"types": "./dist/server/stateless.d.ts",
			"default": "./dist/server/stateless.js"
		},
		"./helpers": {
			"types": "./dist/helpers/index.d.ts",
			"default": "./dist/helpers/index.js"
		},
		"./react": {
			"types": "./dist/react/index.d.ts",
			"default": "./dist/react/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsc",
		"build:all": "npm run build -ws --include-workspace-root",
		"watch": "tsc --watch",
		"check": "npx @biomejs/biome check --write --unsafe"
	},
	"packageManager": "npm@11.4.1",
	"license": "MIT",
	"dependencies": {
		"@modelcontextprotocol/sdk": "^1.18.1",
		"chalk": "^5.6.2",
		"express": "^5.1.0",
		"jose": "^6.1.0",
		"json-schema": "^0.4.0",
		"lodash": "^4.17.21",
		"okay-error": "^1.0.3",
		"uuid": "^11.0.3",
		"zod": "^3.23.8",
		"zod-to-json-schema": "^3.24.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.0.6",
		"@types/express": "^5.0.1",
		"@types/json-schema": "^7.0.15",
		"@types/lodash": "^4.17.17",
		"@types/node": "^20.0.0",
		"@types/react": "^19.2.2",
		"@types/uuid": "^9.0.7",
		"dotenv": "^16.4.7",
		"tsx": "^4.19.2",
		"typescript": "^5.0.0"
	}
}
